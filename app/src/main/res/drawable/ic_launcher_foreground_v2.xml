<?xml version="1.0" encoding="utf-8"?>
<!-- Modern RadioFlow+ foreground icon: Play button with radio waves -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- Play triangle (centered) -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M42,35 L42,73 L72,54 Z" />
    
    <!-- Radio wave 1 (inner) -->
    <path
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2.5"
        android:strokeLineCap="round"
        android:fillColor="@android:color/transparent"
        android:pathData="M72,40 Q82,54 72,68" />
    
    <!-- Radio wave 2 (outer) -->
    <path
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2.5"
        android:strokeLineCap="round"
        android:fillColor="@android:color/transparent"
        android:pathData="M78,32 Q92,54 78,76" />
    
    <!-- Radio wave 3 (outermost) -->
    <path
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:strokeAlpha="0.7"
        android:fillColor="@android:color/transparent"
        android:pathData="M84,24 Q102,54 84,84" />
    
</vector>
